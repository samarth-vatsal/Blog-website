<%- include('partials/header'); -%>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <br>
    <h2>Journal Entry</h2>
    <br>
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle"><br>
    <label>Post</label>
    <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea><br>
  </div>
  <button class="btn btn-primary" type="submit" name="button" >Publish</button>
</form>
<%- include('partials/footer'); -%>
